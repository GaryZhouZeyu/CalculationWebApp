DROP TABLE USER;

CREATE TABLE user(
USERID VARCHAR(10) NOT NULL,
PASSWORD VARCHAR(10) NOT NULL,
PRIMARY KEY(USERID)
);

INSERT INTO user(USERID,PASSWORD) VALUES ('BOB369', 'ABC123'); 


DROP TABLE calculation;

CREATE TABLE calculation(
ID INT NOT NULL AUTO_INCREMENT,
INPUT1 DOUBLE NOT NULL,
INPUT2 DOUBLE NOT NULL,
OPERATOR VARCHAR(1) NOT NULL,
RESULT DOUBLE NOT NULL,
NOTES VARCHAR(100),
SEQUENCE INT NOT NULL,
USERID VARCHAR(10) NOT NULL,
FOREIGN KEY(USERID) REFERENCES USER(USERID),
PRIMARY KEY(ID));

INSERT INTO calculation(INPUT1,INPUT2,OPERATOR,RESULT,NOTES,SEQUENCE,USERID) VALUES (1,1,"+",2,"NA",0,"BOB369"); 

SELECT calculation.INPUT1,calculation.INPUT2,calculation.OPERATOR,calculation.RESULT,calculation.NOTES,calculation.SEQUENCE,user.USERID,user.PASSWORD
FROM calculation
INNER JOIN user ON calculation.USERID=user.USERID;